{
  "name": "nullislanders",
  "version": "0.0.1",
  "description": "HNClone for Geo Nerds",
  "main": "index.js",
  "scripts": {
    "prod": "node ./start.js",
    "watch": "node ./node_modules/nodemon/bin/nodemon.js ./start.js",
    "start": "npm run watch",
    "test": "jest src",
    "drop:postgres:dev": "dropdb nullislanders_dev",
    "drop:postgres:test": "dropdb nullislanders_test",
    "drop:postgres:prod": "dropdb nullislanders",
    "create:postgres:dev": "psql -U postgres -c \"CREATE DATABASE nullislanders_dev\"",
    "create:postgres:test": "psql -U postgres -c \"CREATE DATABASE nullislanders_test\"",
    "create:postgres:prod": "psql -U postgres -c \"CREATE DATABASE nullislanders\"",
    "migrate:dev": "knex migrate:latest --env development",
    "migrate:prod": "knex migrate:latest --env production",
    "migrate:test": "knex migrate:latest --env test",
    "build:dev": "npm run create:postgres:dev && npm run create:postgres:test && npm run migrate:dev && npm run migrate:test"
  },
  "author": "Brian Bancroft",
  "license": "MIT",
  "browserslist": "last 3 versions",
  "dependencies": {
    "axios": "0.15.3",
    "bcrypt": "^3.0.1",
    "body-parser": "1.17.1",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.3",
    "dotenv": "4.0.0",
    "ejs": "^2.6.1",
    "eslint": "^5.6.0",
    "express": "^4.16.3",
    "express-session": "^1.15.1",
    "express-validator": "3.1.2",
    "faker": "4.1.0",
    "jest-express": "^1.8.0",
    "mysql": "^2.16.0",
    "node-time-ago": "^1.0.0",
    "nodemailer": "3.1.5",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "pg": "^7.4.3",
    "request": "^2.88.0",
    "slugs": "0.1.3",
    "uuid": "3.0.1",
    "validator": "7.0.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.0",
    "babel-loader": "6.4.0",
    "babel-preset-env": "^1.6.0",
    "concurrently": "3.4.0",
    "css-loader": "0.28.11",
    "eslint-plugin-import": "^2.14.0",
    "extract-text-webpack-plugin": "2.1.0",
    "jest": "^23.6.0",
    "knex": "^0.15.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.18.3",
    "now": "^6.4.1",
    "postcss-loader": "1.3.3",
    "prettier": "^1.14.3",
    "sass-loader": "6.0.3",
    "webpack": "2.2.1"
  }
}
